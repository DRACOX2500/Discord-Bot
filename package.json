{
	"name": "utsukushi-bot",
	"version": "2.0.0",
	"author": "DRACOX2500",
	"license": "MIT",
	"description": "Discord Bot for a private Discord Server",
	"repository": {
		"url": "https://github.com/DRACOX2500/UTSUKUSHI-Bot"
	},
	"main": "index.ts",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"remove-dist": "echo \"Error: no remove-dist specified\" && exit 1",
		"watch": "tsc --project tsconfig.json --watch",
		"deploy-commands": "ts-node dist/index.js",
		"reset-commands": "ts-node dist/index.js  --guild-id",
		"start": "ts-node dist/index.js",
		"dev": "ts-node dist/index.js --profile dev",
		"build-start": "npm run build && npm run start",
		"super-start": "npm run build && npm run deploy-commands && npm run start",
		"build-dev": "npm run build && npm run dev",
		"super-dev": "npm run build && npm run deploy-commands && npm run dev",
		"nodemon": "nodemon",
		"lint": "echo \"Error: no lint specified\" && exit 1",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"bot",
		"discord",
		"discord-bot",
		"discord-js",
		"javascript",
		"music",
		"nodejs",
		"music",
		"typescript"
	],
	"dependencies": {
		"ansicolor": "^1.1.100",
		"axios": "0.27.2",
		"discord.js": "^14.12.1",
		"dotenv": "^16.3.1",
		"mongoose": "^7.4.2",
		"pino": "^8.15.0",
		"pino-pretty": "^10.2.0",
		"rxjs": "^7.8.1",
		"staticmaps": "^1.12.0"
	},
	"devDependencies": {
		"@types/staticmaps": "^1.12.1",
		"nodemon": "^3.0.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.6"
	}
}
