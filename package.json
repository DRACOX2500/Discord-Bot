{
	"name": "utsukushi-bot",
	"version": "2.0.0",
	"author": "DRACOX2500",
	"license": "MIT",
	"description": "Discord Bot for a private Discord Server",
	"repository": {
		"url": "https://github.com/DRACOX2500/UTSUKUSHI-Bot"
	},
	"engines": {
		"node": ">=16 <19",
		"npm": ">=9 <10"
	},
	"main": "index.ts",
	"scripts": {
		"heroku-cleanup": "yarn && npm run opus-fix && npm run build",
		"build": "tsc --project tsconfig.json",
		"remove-dist": "echo \"Error: no remove-dist specified\" && exit 1",
		"watch": "tsc --project tsconfig.json --watch",
		"deploy-commands": "node dist/index.js",
		"reset-commands": "node dist/index.js  --guild-id",
		"start": "node dist/index.js",
		"dev": "node dist/index.js --profile dev",
		"build-start": "npm run build && npm run start",
		"super-start": "npm run build && npm run deploy-commands && npm run start",
		"build-dev": "npm run build && npm run dev",
		"super-dev": "npm run build && npm run deploy-commands && npm run dev",
		"nodemon": "nodemon",
		"lint": "eslint .",
		"lint-fix": "eslint --fix .",
		"test": "jest",
		"opus-fix": "yarn add @discordjs/opus"
	},
	"keywords": [
		"bot",
		"discord",
		"discord-bot",
		"discord-js",
		"javascript",
		"music",
		"nodejs",
		"music",
		"typescript"
	],
	"dependencies": {
		"@discord-player/extractor": "^4.4.1",
		"@discordjs/opus": "^0.9.0",
		"ansicolor": "^1.1.100",
		"axios": "0.27.2",
		"discord-player": "^6.6.2",
		"discord.js": "^14.12.1",
		"dotenv": "^16.3.1",
		"ffmpeg-static": "^5.2.0",
		"mongoose": "^7.4.2",
		"pino": "^8.15.0",
		"pino-pretty": "^10.2.0",
		"rxjs": "^7.8.1",
		"staticmaps": "^1.12.0",
		"ytdl-core": "^4.11.5"
	},
	"devDependencies": {
		"@jest/globals": "^29.6.4",
		"@types/jest": "^29.5.4",
		"@types/mocha": "^10.0.1",
		"@types/staticmaps": "^1.12.1",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.4.1",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-standard-with-typescript": "^38.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-promise": "^6.0.0",
		"jest": "^29.6.4",
		"nodemon": "^3.0.1",
		"prettier": "3.0.2",
		"ts-jest": "^29.1.1",
		"typescript": "^5.1.6"
	}
}
